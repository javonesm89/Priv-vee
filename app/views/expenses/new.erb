<%= form_for(@expense) do |f| %>
    <%= f.hidden_field :user_id %>
    INCOME: <%= f.collection_select :income_id, @incomes, :id, :source, {:include_blank => true} %><p>
    STRATEGY: <%= f.text_field :source, :placeholder => 'RENT' %><p>
    AMOUNT: <%= f.number_field :amount %>
    <p>OR
    PERCENT: <%= f.number_field :off_percent %>
    <%= f.submit "SUBMIT" %>
<% end %>
<%= content_tag :div, "", :class => "text-center fw-light" do %>
    <%= flash[:alert] if true %>
<% end %>




<%= content_tag :div, "", :class => "container" do %>
            <%= content_tag :p, "Hey. Login.", :class => "text-center display-4" %>
            <img class="rounded mx-auto d-block"  src="data:image/svg+xml;base64,"/>    <%= form_for login_path do %>
            <%= content_tag :div, "", :class => "text-center" do %>
            <%= label_tag :email, "Email", :class => "form-label" %>
            <%= email_field_tag :email, "", :class => "form-control" %>
            <%= label_tag :password, "Password", :class => "form-label" %>
            <%= password_field_tag :password, "", :class => "form-control" %>
            <%= submit_tag "Login" %>
        <% end %>
    <% end %>
<% end %>

